record Person {
    name: string;
    age: int;
}

record Car {
    make: string;
    year: int;
}

int main() {
    var p: Person = new Person { name = "Alice"; age = 30; };
    var c: Car = new Car { make = "Toyota"; year = 2020; };
    if (p == c) {  /* Comparing Person and Car records */
        return 1;
    }
    return 0;
}
