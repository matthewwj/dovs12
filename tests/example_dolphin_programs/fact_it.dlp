void printInt(i : int){
   output_string(int_to_string(i),get_stdout());
   output_string("\n",get_stdout());
}

int fact(n: int) {
    var x = n;
    var res = 1;
    while (x > 1) {
        res = res * x;
        x = x-1;
    };
    return res;
}

int main() {
    printInt(fact(5)); 
    printInt(fact(11)); 
    return 0;
}